                <groupId>cn.wnhyang</groupId>
                <artifactId>coolguard-common</artifactId>
                <version>${project.version}</version>
            </dependency>

            <dependency>
                <groupId>cn.wnhyang</groupId>
                <artifactId>coolguard-system</artifactId>
                <version>${project.version}</version>
            </dependency>

            <dependency>
                <groupId>cn.wnhyang</groupId>
                <artifactId>coolguard-decision</artifactId>
